{
  "buildCommand": "npm run build:studio || true && npx next build",
  "framework": "nextjs",
  "rewrites": [
    {
      "source": "/studio/:path*",
      "destination": "/studio/:path*"
    },
    {
      "source": "/static/:path*",
      "destination": "/static/:path*"
    }
  ],
  "headers": [
    {
      "source": "/studio/:path*",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=3600"
        }
      ]
    }
  ]
}
